{
    "name": "ProductiviTree",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_icon": "icon_1_48x48.png"
    },
    "icons": {
        "48": "icon_1_48x48.png",
        "128": "icon_128x128.png"
    },
    "permissions": [
        "tabs",
        "notifications",
        "storage",
        "history"
      ],

    "chrome_url_overrides" : {
        "newtab": "test.html"
      }
}