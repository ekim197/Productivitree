{
    "name": "bear",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs",
        "notifications",
        "storage"
      ],

    "chrome_url_overrides" : {
        "newtab": "test.html"
      }
}